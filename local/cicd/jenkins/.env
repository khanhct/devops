# Jenkins Settings
JENKINS_LOCAL_HOME=./jenkins_home
JENKINS_UID=1000
JENKINS_GID=1000
HOST_DOCKER_SOCK=/var/run/docker.sock

# Nginx Settings
NGINX_CONF=./nginx/default.conf
NGINX_SSL_CERTS=./ssl
NGINX_LOGS=./logs/nginx

